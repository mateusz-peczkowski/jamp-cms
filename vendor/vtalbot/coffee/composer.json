{
    "name": "vtalbot/coffee",
    "description": "CoffeeScript compiler for Laravel 4 (Illuminate)",
    "keywords": ["laravel", "coffee", "coffeescript", "illuminate"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Talbot",
            "email": "vincent.talbot@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "laravel/framework": ">=4.0",
        "coffeescript/coffeescript": "*"
    },
    "replace": {
        "ellicom/coffee": "*"
    },
    "require-dev": {
        "mockery/mockery": "0.7.2"
    },
    "autoload": {
        "psr-0": {"VTalbot\\Coffee": "src/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}